{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": [
    "object"
  ],
  "description": "Indicates start of an overtime period. Used for NCAAF games for the first OT period, which doesn't start overtime with a kickoff. Optional for other OT periods in NCAA.",
  "title": "start_overtime",
  "allOf": [
    {
      "$ref": "https://txodds.com/schemas/usfootball/common.json"
    },
    {
      "type": [
        "object"
      ],
      "properties": {
        "Action": {
          "type": [
            "string"
          ],
          "description": "Action Type",
          "title": "Action",
          "enum": [
            "start_overtime"
          ]
        },
        "Clock": {
          "type": [
            "object",
            "null"
          ],
          "description": "Game clock",
          "title": "Clock",
          "$ref": "https://txodds.com/schemas/usfootball/common.json#/$defs/Clock"
        },
        "Confirmed": {
          "type": [
            "boolean"
          ],
          "description": "Action confirmation status. This indicates an action that actually happened, as opposed to _possible_ actions, however the final confirmation is still pending and will be sent with the same action ID, and potentially more information related to the action. An unconfirmed event is closer in time to the event happening, as confirmation could be delayed by events on field. Take this in account when handling the events",
          "title": "Confirmed",
          "examples": [
            false,
            true
          ]
        },
        "Data": {
          "type": [
            "object"
          ],
          "description": "Action specific info",
          "title": "Data",
          "properties": {
            "Participant": {
              "type": [
                "number"
              ],
              "description": "The team that starts the overtime period.",
              "title": "Participant",
              "examples": [
                1,
                2
              ]
            }
          },
          "additionalProperties": false,
          "required": [
            "Participant"
          ]
        },
        "Down": {
          "type": [
            "object",
            "null"
          ],
          "description": "Current down info",
          "title": "Down",
          "$ref": "https://txodds.com/schemas/usfootball/common.json#/$defs/Down"
        },
        "InPlayInfo": {
          "type": [
            "object",
            "null"
          ],
          "description": "Data about current play state",
          "title": "InPlayInfo",
          "$ref": "https://txodds.com/schemas/usfootball/common.json#/$defs/InPlayInfo"
        },
        "KickoffInfo": {
          "type": [
            "object",
            "null"
          ],
          "description": "Kick-off information",
          "title": "KickoffInfo",
          "$ref": "https://txodds.com/schemas/usfootball/common.json#/$defs/KickoffInfo"
        },
        "Parti1State": {
          "type": [
            "object",
            "null"
          ],
          "description": "State for the `Participant1`. Home/away mapping is provided by the `Participant1IsHome` flag on the fixture",
          "title": "Parti1State",
          "$ref": "https://txodds.com/schemas/usfootball/common.json#/$defs/PartiState"
        },
        "Parti2State": {
          "type": [
            "object",
            "null"
          ],
          "description": "State for the `Participant2`. Home/away mapping is provided by the `Participant1IsHome` flag on the fixture",
          "title": "Parti2State",
          "$ref": "https://txodds.com/schemas/usfootball/common.json#/$defs/PartiState"
        }
      }
    }
  ],
  "unevaluatedProperties": false,
  "required": [
    "Action",
    "Confirmed",
    "ConnectionId",
    "Data",
    "Down",
    "FixtureId",
    "Id",
    "InPlayInfo",
    "KickoffInfo",
    "Seq",
    "StatusId",
    "Ts",
    "Type"
  ]
}
