{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": [
    "object"
  ],
  "description": "Team action backward pass indicating start of the play.\nThis action is *confirmed automatically* and will not be followed up with a confirmation message.",
  "title": "snap",
  "allOf": [
    {
      "$ref": "https://txodds.com/schemas/usfootball/common.json"
    },
    {
      "type": [
        "object"
      ],
      "properties": {
        "Action": {
          "type": [
            "string"
          ],
          "description": "Action Type",
          "title": "Action",
          "enum": [
            "snap"
          ]
        },
        "Clock": {
          "type": [
            "object",
            "null"
          ],
          "description": "Game clock",
          "title": "Clock",
          "$ref": "https://txodds.com/schemas/usfootball/common.json#/$defs/Clock"
        },
        "Down": {
          "type": [
            "object",
            "null"
          ],
          "description": "Current down info",
          "title": "Down",
          "$ref": "https://txodds.com/schemas/usfootball/common.json#/$defs/Down"
        },
        "InPlayInfo": {
          "type": [
            "object",
            "null"
          ],
          "description": "Data about current play state",
          "title": "InPlayInfo",
          "$ref": "https://txodds.com/schemas/usfootball/common.json#/$defs/InPlayInfo"
        }
      }
    }
  ],
  "unevaluatedProperties": false,
  "required": [
    "Action",
    "Clock",
    "ConnectionId",
    "FixtureId",
    "Id",
    "Seq",
    "StatusId",
    "Ts",
    "Type"
  ]
}
