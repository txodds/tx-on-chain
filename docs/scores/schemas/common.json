    "ScoreParticipant": {
      "type": [
        "object"
      ],
      "description": "Score information for a participant",
      "title": "Score for Participant in Period",
      "properties": {
        "HT": {
          "type": [
            "object",
            "null"
          ],
          "description": "Score up to halftime",
          "title": "Half Time Score",
          "$ref": "#/$defs/ScoreParticipantPeriod"
        },
        "OT": {
          "type": [
            "object",
            "null"
          ],
          "description": "Overtime score, the key indicating the overtime number and will have as many keys as there were overtimes in the game",
          "title": "Overtime Score",
          "patternProperties": {
            "^[0-9]+$": {
              "type": [
                "object"
              ],
              "description": "Overtime score for the corresponding overtime",
              "$ref": "#/$defs/ScoreParticipantPeriod"
            }
          }
        },
        "OTTotal": {
          "type": [
            "object",
            "null"
          ],
          "description": "Total overtime score: the sum of scores in all overtimes",
