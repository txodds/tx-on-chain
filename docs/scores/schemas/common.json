{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "https://txodds.com/schemas/usfootball/common.json",
  "type": [
    "object"
  ],
  "description": "Format of fields that can appear in all Fixture Actions",
  "title": "Common Fields Schema",
  "properties": {
    "ConnectionId": {
      "type": [
        "number"
      ],
      "description": "Internal ID of the connection",
      "title": "ConnectionId",
      "examples": [
        12,
        13,
        9,
        1,
        18
      ]
    },
    "FixtureId": {
      "type": [
        "number"
      ],
      "description": "The normative Id of the fixture",
      "title": "FixtureId",
      "examples": [
        13702627,
        13491872,
        13196850,
        13911618,
        14790382
      ]
    },
    "GlobalSeq": {
      "type": [
        "number"
      ],
      "description": "Global update sequence. When tracking event sequence numbers, use `Seq` instead.",
      "title": "Seq",
      "examples": [
        152,
        60,
        74,
        40,
        8
      ]
    },
    "Id": {
      "type": [
        "number"
      ],
      "description": "Action ID. Messages for the same action will have the same action ID. For example, an unconfirmed action followed by a confirmation",
      "title": "Id",
      "examples": [
        244,
        105,
        175,
        170,
        36
      ]
    },
    "Seq": {
      "type": [
        "number"
      ],
      "description": "Update sequence number for a fixture",
      "title": "Seq",
      "examples": [
        155,
        97,
        130,
        124,
        103
      ]
    },
    "ServerId": {
      "type": [
        "string"
      ],
      "description": "Internal ID of the server that handled the action",
      "title": "Server Id",
      "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
      "examples": [
        "e42898ee-2051-4d1e-8817-afc3f7cc156c",
        "227264fb-4132-4e49-9121-3165c753002e"
      ]
    },
    "TracingInfo": {
      "type": [
        "object"
      ],
      "description": "Observability information for the action. Relevant if issues are reported.",
      "title": "Tracing Information"
    },
    "Ts": {
      "type": [
        "number"
      ],
      "description": "Timestamp of the update",
      "title": "Ts",
      "examples": [
        1718872931588,
        1719405509055,
        1719320331078,
        1719320244083,
        1718823346736
      ]
    },
    "Type": {
      "type": [
        "string"
      ],
      "description": "Sport Type",
      "title": "Type",
      "enum": [
        "UsFootball"
      ]
    },
    "UserId": {
      "type": [
        "string"
      ],
      "description": "Internal ID of the user that sent the action",
      "title": "Server Id",
      "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
      "examples": [
        "e42898ee-2051-4d1e-8817-afc3f7cc156c",
        "227264fb-4132-4e49-9121-3165c753002e"
      ]
    },
    "VirtualFixture": {
      "type": [
        "null",
        "boolean"
      ],
      "description": "If true, the fixture is a virtual fixture, used to replay events of an existing fixture for test purposes.",
      "title": "VirtualFixture",
      "examples": [
        null,
        false,
        true
      ]
    }
  },
  "$defs": {
    "Clock": {
      "type": [
        "object",
        "null"
      ],
      "description": "Game clock",
      "title": "Clock",
      "properties": {
        "Running": {
          "type": [
            "boolean"
          ],
          "description": "Is the clock currently running?",
          "title": "Running",
          "examples": [
            false,
            true
          ]
        },
        "Seconds": {
          "type": [
            "number"
          ],
          "description": "Time in seconds left in the current period. At the start it is the full time for the period (for example, 15 minutes would be 900 seconds). The value decreases with every new action until the end of the period.",
          "title": "Seconds",
          "examples": [
            0,
            302,
            400,
            734,
            900
          ]
        }
      },
      "additionalProperties": false,
      "required": [
        "Running",
        "Seconds"
      ]
    },
    "PlayerData": {
      "type": [
        "object"
      ],
      "description": "Data about a player in a team",
      "title": "Player Data",
      "properties": {
        "country": {
          "type": [
            "string",
            "null"
          ],
          "description": "Player country of origin",
          "title": "Country",
          "examples": [
            null,
            "Dominican Republic",
            "England",
            "Spain",
            "USA"
          ]
        },
        "dateOfBirth": {
          "type": [
            "string",
            "null"
          ],
          "description": "Player date of birth",
          "title": "Date of Birth",
          "examples": [
            null,
            "1987−02−20",
            "1999−12−03",
            "2003−06−11"
          ]
        },
        "entityStatus": {
          "type": [
            "string",
            "null"
          ],
          "description": "Player status",
          "title": "Player status",
          "examples": []
        },
        "entityVersion": {
          "type": [
            "number"
          ],
          "description": "Player information version",
          "title": "Version",
          "examples": [
            0,
            5,
            8
          ]
        },
        "gender": {
          "type": [
            "string"
          ],
          "description": "Gender of the player",
          "title": "Gender",
          "enum": [
            "female",
            "male"
          ]
        },
        "id": {
          "type": [
            "string"
          ],
          "description": "Player UUID",
          "title": "Id",
          "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
          "examples": [
            "227264fb-4132-4e49-9121-3165c753002e",
            "443653c4-32ae-401e-b5ef-97db6fa3ed86"
          ]
        },
        "normativeId": {
          "type": [
            "number"
          ],
          "description": "Player normative Id",
          "title": "Normative Id",
          "examples": [
            1752,
            4268,
            4707
          ]
        },
        "preferredName": {
          "type": [
            "string"
          ],
          "description": "Player name (Last Name, First Name) in normalised Latin chars",
          "title": "Player Name",
          "examples": [
            "Baker, Brandon",
            "Gallian, John",
            "Kornet, Luke"
          ]
        },
        "sportId": {
          "type": [
            "string"
          ],
          "description": "Sport Id",
          "title": "Sport Id",
          "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
          "examples": [
            "00000000-0000-0006-0000-000000000003",
            "00000000-0000-0006-0000-000000000004"
          ]
        },
        "team": {
          "type": [
            "string",
            "null"
          ],
          "description": "Player team Id",
          "title": "Team Id",
          "examples": [
            null,
            "00000000-0000-0006-0000-000000000003",
            "00000000-0000-0006-0000-000000000004"
          ]
        },
        "updateDateMillis": {
          "type": [
            "number"
          ],
          "description": "Player update timestamp",
          "title": "Update timestamp",
          "examples": [
            1732878222979,
            1732878726834
          ]
        }
      },
      "additionalProperties": false,
      "required": [
        "entityVersion",
        "gender",
        "id",
        "normativeId",
        "preferredName",
        "sportId",
        "updateDateMillis"
      ]
    },
    "PlayerLineupData": {
      "type": [
        "object"
      ],
      "description": "Lineup information for a team",
      "title": "Player Lineup Data",
      "properties": {
        "fixturePlayerId": {
          "type": [
            "number"
          ],
          "description": "Player Id in the fixture, used to reference a player in events",
          "title": "Fixture Player Id",
          "examples": [
            1187,
            1975,
            5629
          ]
        },
        "player": {
          "type": [
            "object",
            "null"
          ],
          "description": "Player information",
          "title": "Player",
          "$ref": "#/$defs/PlayerData"
        },
        "positionId": {
          "type": [
            "number"
          ],
          "description": "Position Id in the fixture",
          "title": "Position Id",
          "examples": [
            11,
            34,
            37
          ]
        },
        "rosterNumber": {
          "type": [
            "string",
            "null"
          ],
          "description": "Roster number",
          "title": "Number",
          "examples": [
            null,
            "0",
            "00",
            "09",
            "9",
            "18"
          ]
        },
        "starter": {
          "type": [
            "boolean"
          ],
          "description": "The player is part of the starting team",
          "title": "Starter",
          "examples": [
            true,
            false
          ]
        },
        "starred": {
          "type": [
            "boolean"
          ],
          "description": "The player is starred in the lineup",
          "title": "Starred",
          "examples": [
            true,
            false
          ]
        },
        "statusId": {
          "type": [
            "number"
          ],
          "description": "Player Status",
          "title": "Status",
          "examples": [
            3,
            7
          ]
        },
        "unitId": {
          "type": [
            "number"
          ],
          "description": "Unit Id",
          "title": "Unit",
          "examples": [
            2,
            5
          ]
        }
      },
      "additionalProperties": false,
      "required": [
        "positionId",
        "statusId",
        "unitId"
      ]
    },
    "LineupData": {
      "type": [
        "object"
      ],
      "description": "Lineup information for a team",
      "title": "Lineup Data",
      "properties": {
        "entityStatus": {
          "type": [
            "string",
            "null"
          ],
          "description": "Team status",
          "title": "Team status",
          "examples": []
        },
        "entityVersion": {
          "type": [
            "number"
          ],
          "description": "Team information version",
          "title": "Version",
          "examples": [
            0,
            5,
            8
          ]
        },
        "gender": {
          "type": [
            "string"
          ],
          "description": "Information about the gender of the team (which gendered competitions it is playing in)",
          "title": "Team Gender",
          "enum": [
            "female",
            "male"
          ]
        },
        "id": {
          "type": [
            "string"
          ],
          "description": "Team UUID",
          "title": "Id",
          "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
          "examples": [
            "227264fb-4132-4e49-9121-3165c753002e",
            "443653c4-32ae-401e-b5ef-97db6fa3ed86"
          ]
        },
        "lineups": {
          "type": [
            "array"
          ],
          "description": "Player lineup information",
          "title": "Player lineup",
          "items": {
            "$ref": "#/$defs/PlayerLineupData"
          }
        },
        "normativeId": {
          "type": [
            "number"
          ],
          "description": "Team normative Id",
          "title": "Normative Id",
          "examples": [
            4268,
            4707
          ]
        },
        "preferredName": {
          "type": [
            "string"
          ],
          "description": "Team name",
          "title": "Team Name",
          "examples": [
            "Boston Celtics",
            "Chicago Bulls"
          ]
        },
        "sportId": {
          "type": [
            "string"
          ],
          "description": "Sport Id",
          "title": "Sport Id",
          "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
          "examples": [
            "00000000-0000-0006-0000-000000000003",
            "00000000-0000-0006-0000-000000000004"
          ]
        },
        "updateDateMillis": {
          "type": [
            "number"
          ],
          "description": "Team update timestamp",
          "title": "Update timestamp",
          "examples": [
            1732878222979,
            1732878726834
          ]
        }
      },
      "additionalProperties": false,
      "required": [
        "entityVersion",
        "gender",
        "id",
        "lineups",
        "normativeId",
        "preferredName",
        "sportId",
        "updateDateMillis"
      ]
    },
