{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": [
    "object"
  ],
  "description": "Indicates the timeout called by the teams or officials is over.\nThis action is *confirmed automatically* and will not be followed up with a confirmation message.",
  "title": "timeout_ended",
  "allOf": [
    {
      "$ref": "https://txodds.com/schemas/usfootball/common.json"
    },
    {
      "type": [
        "object"
      ],
      "properties": {
        "Action": {
          "type": [
            "string"
          ],
          "description": "Action Type",
          "title": "Action",
          "enum": [
            "timeout_ended"
          ]
        },
        "Clock": {
          "type": [
            "object",
            "null"
          ],
          "description": "Game clock",
          "title": "Clock",
          "$ref": "https://txodds.com/schemas/usfootball/common.json#/$defs/Clock"
        },
        "Data": {
          "type": [
            "object"
          ],
          "description": "Action specific info",
          "title": "Data",
          "properties": {
            "Participant": {
              "type": [
                "number"
              ],
              "description": "Team that calls the timeout. Can be 0, indicating a timeout by officials (neutral)",
              "title": "Participant",
              "examples": [
                1,
                2,
                0
              ]
            }
          },
          "additionalProperties": false,
          "required": [
            "Participant"
          ]
        },
        "InPlayInfo": {
          "type": [
            "object"
          ],
          "description": "Data about current play state",
          "title": "InPlayInfo",
          "$ref": "https://txodds.com/schemas/usfootball/common.json#/$defs/InPlayInfo"
        },
        "OvertimeNumber": {
          "type": [
            "number",
            "null"
          ],
          "description": "Additional field included only for Overtime periods. The value is the current Overtime",
          "title": "OvertimeNumber",
          "examples": [
            null,
            1
          ]
        },
        "StatusId": {
          "type": [
            "number"
          ],
          "description": "The ID for the current game period. See [StatusId](#status-id)",
          "title": "StatusId",
          "examples": [
            11,
            2,
            4,
            9,
            12
          ]
        }
      }
    }
  ],
  "unevaluatedProperties": false,
  "required": [
    "Action",
    "Clock",
    "ConnectionId",
    "Data",
    "FixtureId",
    "Id",
    "InPlayInfo",
    "Seq",
    "StatusId",
    "Ts",
    "Type"
  ]
}
